<LPRDefinition> 
  <LPR Name="cxo" CoreMask="0xFFFFFFFF" InitFn="cxo_shutdown_init" ProfileFreq="245760">
    <Mode Name="shutdown"
          Dependency="rpm.sync + cpu_vdd"
          Order="BEFORE DEP cxo.shutdown"
          PowerSavings="0, 290"
          EnterLat="4992,0"
          ExitLat="5316,0"
          Backoff="516,0"
          EnterFn="cxo_shutdown_enter"
          ExitFn="cxo_shutdown_exit"
          >
    </Mode>
  </LPR>
</LPRDefinition>
