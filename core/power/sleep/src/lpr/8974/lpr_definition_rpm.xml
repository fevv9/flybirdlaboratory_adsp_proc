<LPRDefinition>
  <LPR Name="rpm" CoreMask="0xFFFFFFFF" InitFn="rpm_sync_init" ProfileFreq="245760">
    <Mode Name="sync"
          Dependency="cpu_vdd"
          Order="AFTER DEPON rpm.sync"
          PowerSavings="0"
          EnterLat="26346"
          ExitLat="25158"
          Backoff="1158"
          EnterFn="rpm_sync_enter"
          ExitFn="rpm_sync_exit"
          >
    </Mode>
  </LPR>
</LPRDefinition>
